<resources>

  <!-- PlatLogo -->
  <java-symbol type="drawable" name="platlogo_toxyc" />

  <!-- Forcefully tint notification icon text -->
  <java-symbol type="bool" name="config_useDarkBgNotificationIconTextTinting" />

  <!-- Extended Power Menu -->
  <java-symbol type="array" name="config_extendedGlobalActionsList" />
  <java-symbol type="drawable" name="ic_restart_quick" />
  <java-symbol type="drawable" name="ic_restart_recovery" />
  <java-symbol type="drawable" name="ic_restart_bootloader" />
  <!-- Defined in res/res/values/symbols.xml -
  <java-symbol type="string" name="global_action_restart" />-->
  <java-symbol type="string" name="global_action_quick_restart" />
  <java-symbol type="string" name="global_action_recovery" />
  <java-symbol type="string" name="global_action_bootloader" />
  <!-- Shutdown Thread -->
  <java-symbol type="string" name="global_restart" />
  <java-symbol type="string" name="global_restart_message" />
  <java-symbol type="string" name="global_quick_restart" />
  <java-symbol type="string" name="global_restart_recovery" />
  <java-symbol type="string" name="global_restart_recovery_message" />
  <java-symbol type="string" name="global_restart_bootloader" />
  <java-symbol type="string" name="global_restart_bootloader_message" />

  <!-- Allow SystemUI to show the shutdown confirm dialog -->
  <java-symbol type="bool" name="config_showSysuiConfirmShutdown" />
  <java-symbol type="id" name="confirm_message"/>
  <java-symbol type="id" name="confirm_yes"/>
  <java-symbol type="id" name="confirm_no"/>
  <java-symbol type="layout" name="shutdown_confirm_dialog" />
  <java-symbol type="string" name="global_shutdown_confirm_message" />
  <java-symbol type="string" name="global_restart_confirm_message" />
  <java-symbol type="string" name="global_restart_recovery_confirm_message" />
  <java-symbol type="string" name="global_restart_bootloader_confirm_message" />

  <!-- Doze Addon: should the TYPE_PROXIMITY sensor be used as a pulse signal. -->
  <java-symbol type="bool" name="config_dozePulseHandWave" />

  <!-- Custom Analog clock -->
  <java-symbol type="drawable" name="custom_clock_dial" />
  <java-symbol type="drawable" name="custom_clock_dial_ambient" />
  <java-symbol type="drawable" name="custom_clock_hand_hour" />
  <java-symbol type="drawable" name="custom_clock_hand_minute" />
  <java-symbol type="color" name="analog_clock_border_color" />
  <java-symbol type="color" name="analog_clock_face_color" />
  <java-symbol type="color" name="analog_clock_hand_hour_color" />
  <java-symbol type="color" name="analog_clock_hand_minute_color" />

  <!-- DeadPool Analog clock -->
  <java-symbol type="drawable" name="deadpool_clock_dial" />
  <java-symbol type="drawable" name="deadpool_clock_dial_ambient" />
  <java-symbol type="drawable" name="deadpool_clock_hand_hour" />
  <java-symbol type="drawable" name="deadpool_clock_hand_minute" />

  <!-- System fonts -->
  <java-symbol type="array" name="config_fontPackages" />

  <!-- HW keys binding -->  
  <java-symbol type="anim" name="custom_app_in" />
  <java-symbol type="anim" name="custom_app_out" />
  <java-symbol type="integer" name="config_deviceHardwareKeys" />
  <java-symbol type="integer" name="config_deviceHardwareWakeKeys" />

</resources>